{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "height": 600,
        "title": "Airi",
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "com.github.iarlo",
  "plugins": {
    "sql": {
      "preload": [
        "sqlite:airi.db"
      ]
    }
  },
  "productName": "Airi",
  "version": "0.0.0"
}